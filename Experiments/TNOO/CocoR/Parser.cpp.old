#include <wchar.h>
#include "Parser.h"
#include "Scanner.h"




void Parser::SynErr(int n) {
	if (errDist >= minErrDist) errors->SynErr(la->line, la->col, n);
	errDist = 0;
}

void Parser::SemErr(const wchar_t* msg) {
	if (errDist >= minErrDist) errors->Error(t->line, t->col, msg);
	errDist = 0;
}

void Parser::Get() {
	for (;;) {
		t = la;
		la = scanner->Scan();
		if (la->kind <= maxT) { ++errDist; break; }

		if (dummyToken != t) {
			dummyToken->kind = t->kind;
			dummyToken->pos = t->pos;
			dummyToken->col = t->col;
			dummyToken->line = t->line;
			dummyToken->next = NULL;
			coco_string_delete(dummyToken->val);
			dummyToken->val = coco_string_create(t->val);
			t = dummyToken;
		}
		la = t;
	}
}

void Parser::Expect(int n) {
	if (la->kind==n) Get(); else { SynErr(n); }
}

void Parser::ExpectWeak(int n, int follow) {
	if (la->kind == n) Get();
	else {
		SynErr(n);
		while (!StartOf(follow)) Get();
	}
}

bool Parser::WeakSeparator(int n, int syFol, int repFol) {
	if (la->kind == n) {Get(); return true;}
	else if (StartOf(repFol)) {return false;}
	else {
		SynErr(n);
		while (!(StartOf(syFol) || StartOf(repFol) || StartOf(0))) {
			Get();
		}
		return StartOf(syFol);
	}
}

void Parser::number() {
		if (la->kind == _integer) {
			Get();
		} else if (la->kind == _real) {
			Get();
		} else SynErr(67);
}

void Parser::Module() {
		Expect(6 /* "MODULE" */);
		Expect(_ident);
		Expect(7 /* ";" */);
		if (la->kind == 11 /* "IMPORT" */) {
			ImportList();
		}
		DeclSeq();
		if (la->kind == 8 /* "BEGIN" */) {
			Get();
			StatementSeq();
		}
		Expect(9 /* "END" */);
		Expect(_ident);
		Expect(10 /* "." */);
}

void Parser::ImportList() {
		Expect(11 /* "IMPORT" */);
		if (la->kind == _ident) {
			Get();
			Expect(12 /* ":=" */);
		}
		Expect(_ident);
		while (la->kind == 13 /* "," */) {
			Get();
			if (la->kind == _ident) {
				Get();
				Expect(12 /* ":=" */);
			}
			Expect(_ident);
		}
		Expect(7 /* ";" */);
}

void Parser::DeclSeq() {
		while (la->kind == 14 /* "CONST" */ || la->kind == 15 /* "TYPE" */ || la->kind == 16 /* "VAR" */) {
			if (la->kind == 14 /* "CONST" */) {
				Get();
				while (la->kind == _ident) {
					ConstDecl();
					Expect(7 /* ";" */);
				}
			} else if (la->kind == 15 /* "TYPE" */) {
				Get();
				while (la->kind == _ident) {
					TypeDecl();
					Expect(7 /* ";" */);
				}
			} else {
				Get();
				while (la->kind == _ident) {
					VarDecl();
					Expect(7 /* ";" */);
				}
			}
		}
		while (la->kind == 19 /* "PROCEDURE" */) {
			if (la->kind == 19 /* "PROCEDURE" */) {
				ProcDecl();
				Expect(7 /* ";" */);
			} else {
				ForwardDecl();
				Expect(7 /* ";" */);
			}
		}
}

void Parser::StatementSeq() {
		Statement();
		while (la->kind == 7 /* ";" */) {
			Get();
			Statement();
		}
}

void Parser::ConstDecl() {
		IdentDef();
		Expect(17 /* "=" */);
		ConstExpr();
}

void Parser::TypeDecl() {
		IdentDef();
		Expect(17 /* "=" */);
		Type();
}

void Parser::VarDecl() {
		IdentList();
		Expect(18 /* ":" */);
		Type();
}

void Parser::ProcDecl() {
		Expect(19 /* "PROCEDURE" */);
		if (la->kind == 21 /* "(" */) {
			Receiver();
		}
		IdentDef();
		if (la->kind == 21 /* "(" */) {
			FormalPars();
		}
		Expect(7 /* ";" */);
		DeclSeq();
		if (la->kind == 8 /* "BEGIN" */) {
			Get();
			StatementSeq();
		}
		Expect(9 /* "END" */);
		Expect(_ident);
}

void Parser::ForwardDecl() {
		Expect(19 /* "PROCEDURE" */);
		Expect(20 /* "^" */);
		if (la->kind == 21 /* "(" */) {
			Receiver();
		}
		IdentDef();
		if (la->kind == 21 /* "(" */) {
			FormalPars();
		}
}

void Parser::IdentDef() {
		Expect(_ident);
		if (la->kind == 46 /* "-" */ || la->kind == 59 /* "*" */) {
			if (la->kind == 59 /* "*" */) {
				Get();
			} else {
				Get();
			}
		}
}

void Parser::ConstExpr() {
		Expr();
}

void Parser::Type() {
		if (la->kind == _ident) {
			Qualident();
		} else if (la->kind == 23 /* "ARRAY" */) {
			Get();
			if (StartOf(1)) {
				ConstExpr();
				while (la->kind == 13 /* "," */) {
					Get();
					ConstExpr();
				}
			}
			Expect(24 /* "OF" */);
			Type();
		} else if (la->kind == 25 /* "RECORD" */) {
			Get();
			if (la->kind == 21 /* "(" */) {
				Get();
				Qualident();
				Expect(22 /* ")" */);
			}
			FieldList();
			while (la->kind == 7 /* ";" */) {
				Get();
				FieldList();
			}
			Expect(9 /* "END" */);
		} else if (la->kind == 26 /* "POINTER" */) {
			Get();
			Expect(27 /* "TO" */);
			Type();
		} else if (la->kind == 19 /* "PROCEDURE" */) {
			Get();
			if (la->kind == 21 /* "(" */) {
				FormalPars();
			}
		} else SynErr(68);
}

void Parser::IdentList() {
		IdentDef();
		while (la->kind == 13 /* "," */) {
			Get();
			IdentDef();
		}
}

void Parser::Receiver() {
		Expect(21 /* "(" */);
		if (la->kind == 16 /* "VAR" */) {
			Get();
		}
		Expect(_ident);
		Expect(18 /* ":" */);
		Expect(_ident);
		Expect(22 /* ")" */);
}

void Parser::FormalPars() {
		Expect(21 /* "(" */);
		if (la->kind == _ident || la->kind == 16 /* "VAR" */) {
			FPSection();
			while (la->kind == 7 /* ";" */) {
				Get();
				FPSection();
			}
		}
		Expect(22 /* ")" */);
		if (la->kind == 18 /* ":" */) {
			Get();
			Qualident();
			while (la->kind == 13 /* "," */) {
				Get();
				Qualident();
			}
		}
}

void Parser::FPSection() {
		if (la->kind == 16 /* "VAR" */) {
			Get();
		}
		Expect(_ident);
		while (la->kind == 13 /* "," */) {
			Get();
			Expect(_ident);
		}
		Expect(18 /* ":" */);
		Type();
}

void Parser::Qualident() {
		if (la->kind == _ident) {
			Get();
			Expect(10 /* "." */);
		}
		Expect(_ident);
}

void Parser::FieldList() {
		if (la->kind == _ident) {
			IdentList();
			Expect(18 /* ":" */);
			Type();
		}
}

void Parser::Statement() {
		if (StartOf(2)) {
			if (la->kind == _ident) {
				Designator();
				while (la->kind == 13 /* "," */) {
					Get();
					Designator();
				}
				Expect(12 /* ":=" */);
				Expr();
			} else if (la->kind == _ident) {
				Designator();
				if (la->kind == 21 /* "(" */) {
					Get();
					if (StartOf(1)) {
						ExprList();
					}
					Expect(22 /* ")" */);
				}
			} else if (la->kind == 28 /* "IF" */) {
				Get();
				Expr();
				Expect(29 /* "THEN" */);
				StatementSeq();
				while (la->kind == 30 /* "ELSIF" */) {
					Get();
					Expr();
					Expect(29 /* "THEN" */);
					StatementSeq();
				}
				if (la->kind == 31 /* "ELSE" */) {
					Get();
					StatementSeq();
				}
				Expect(9 /* "END" */);
			} else if (la->kind == 32 /* "CASE" */) {
				Get();
				Expr();
				Expect(24 /* "OF" */);
				Case();
				while (la->kind == 33 /* "|" */) {
					Get();
					Case();
				}
				if (la->kind == 31 /* "ELSE" */) {
					Get();
					StatementSeq();
				}
				Expect(9 /* "END" */);
			} else if (la->kind == 34 /* "WHILE" */) {
				Get();
				Expr();
				Expect(35 /* "DO" */);
				StatementSeq();
				Expect(9 /* "END" */);
			} else if (la->kind == 36 /* "REPEAT" */) {
				Get();
				StatementSeq();
				Expect(37 /* "UNTIL" */);
				Expr();
			} else if (la->kind == 38 /* "FOR" */) {
				Get();
				Expect(_ident);
				Expect(12 /* ":=" */);
				Expr();
				Expect(27 /* "TO" */);
				Expr();
				if (la->kind == 39 /* "BY" */) {
					Get();
					ConstExpr();
				}
				Expect(35 /* "DO" */);
				StatementSeq();
				Expect(9 /* "END" */);
			} else if (la->kind == 40 /* "LOOP" */) {
				Get();
				StatementSeq();
				Expect(9 /* "END" */);
			} else if (la->kind == 41 /* "WITH" */) {
				Get();
				Guard();
				Expect(35 /* "DO" */);
				StatementSeq();
				while (la->kind == 33 /* "|" */) {
					Get();
					Guard();
					Expect(35 /* "DO" */);
					StatementSeq();
				}
				if (la->kind == 31 /* "ELSE" */) {
					Get();
					StatementSeq();
				}
				Expect(9 /* "END" */);
			} else if (la->kind == 42 /* "EXIT" */) {
				Get();
			} else {
				Get();
				if (StartOf(1)) {
					Expr();
					while (la->kind == 13 /* "," */) {
						Get();
						Expr();
					}
				}
			}
		}
}

void Parser::Designator() {
		Qualident();
		while (StartOf(3)) {
			if (la->kind == 10 /* "." */) {
				Get();
				Expect(_ident);
			} else if (la->kind == 64 /* "[" */) {
				Get();
				ExprList();
				Expect(65 /* "]" */);
			} else if (la->kind == 20 /* "^" */) {
				Get();
			} else {
				Get();
				Qualident();
				Expect(22 /* ")" */);
			}
		}
}

void Parser::Expr() {
		SimpleExpr();
		if (StartOf(4)) {
			Relation();
			SimpleExpr();
		}
}

void Parser::ExprList() {
		Expr();
		while (la->kind == 13 /* "," */) {
			Get();
			Expr();
		}
}

void Parser::Case() {
		if (StartOf(1)) {
			CaseLabels();
			while (la->kind == 13 /* "," */) {
				Get();
				CaseLabels();
			}
			Expect(18 /* ":" */);
			StatementSeq();
		}
}

void Parser::Guard() {
		Qualident();
		Expect(18 /* ":" */);
		Qualident();
}

void Parser::CaseLabels() {
		ConstExpr();
		if (la->kind == 44 /* ".." */) {
			Get();
			ConstExpr();
		}
}

void Parser::SimpleExpr() {
		if (la->kind == 45 /* "+" */ || la->kind == 46 /* "-" */) {
			if (la->kind == 45 /* "+" */) {
				Get();
			} else {
				Get();
			}
		}
		Term();
		while (la->kind == 45 /* "+" */ || la->kind == 46 /* "-" */ || la->kind == 58 /* "OR" */) {
			AddOp();
			Term();
		}
}

void Parser::Relation() {
		switch (la->kind) {
		case 17 /* "=" */: {
			Get();
			break;
		}
		case 51 /* "#" */: {
			Get();
			break;
		}
		case 52 /* "<" */: {
			Get();
			break;
		}
		case 53 /* "<=" */: {
			Get();
			break;
		}
		case 54 /* ">" */: {
			Get();
			break;
		}
		case 55 /* ">=" */: {
			Get();
			break;
		}
		case 56 /* "IN" */: {
			Get();
			break;
		}
		case 57 /* "IS" */: {
			Get();
			break;
		}
		default: SynErr(69); break;
		}
}

void Parser::Term() {
		Factor();
		while (StartOf(5)) {
			MulOp();
			Factor();
		}
}

void Parser::AddOp() {
		if (la->kind == 45 /* "+" */) {
			Get();
		} else if (la->kind == 46 /* "-" */) {
			Get();
		} else if (la->kind == 58 /* "OR" */) {
			Get();
		} else SynErr(70);
}

void Parser::Factor() {
		switch (la->kind) {
		case _ident: {
			Designator();
			if (la->kind == 21 /* "(" */) {
				Get();
				if (StartOf(1)) {
					ExprList();
				}
				Expect(22 /* ")" */);
			}
			break;
		}
		case _integer: case _real: {
			number();
			break;
		}
		case _character: {
			Get();
			break;
		}
		case _string: {
			Get();
			break;
		}
		case 47 /* "NIL" */: {
			Get();
			break;
		}
		case 49 /* "{" */: {
			Set();
			break;
		}
		case 21 /* "(" */: {
			Get();
			Expr();
			Expect(22 /* ")" */);
			break;
		}
		case 48 /* "~" */: {
			Get();
			Factor();
			break;
		}
		default: SynErr(71); break;
		}
}

void Parser::MulOp() {
		if (la->kind == 59 /* "*" */) {
			Get();
		} else if (la->kind == 60 /* "/" */) {
			Get();
		} else if (la->kind == 61 /* "DIV" */) {
			Get();
		} else if (la->kind == 62 /* "MOD" */) {
			Get();
		} else if (la->kind == 63 /* "&" */) {
			Get();
		} else SynErr(72);
}

void Parser::Set() {
		Expect(49 /* "{" */);
		if (StartOf(1)) {
			Element();
			while (la->kind == 13 /* "," */) {
				Get();
				Element();
			}
		}
		Expect(50 /* "}" */);
}

void Parser::Element() {
		Expr();
		if (la->kind == 44 /* ".." */) {
			Get();
			Expr();
		}
}

void Parser::TNOO() {
		Module();
}




// If the user declared a method Init and a mehtod Destroy they should
// be called in the contructur and the destructor respctively.
//
// The following templates are used to recognize if the user declared
// the methods Init and Destroy.

template<typename T>
struct ParserInitExistsRecognizer {
	template<typename U, void (U::*)() = &U::Init>
	struct ExistsIfInitIsDefinedMarker{};

	struct InitIsMissingType {
		char dummy1;
	};
	
	struct InitExistsType {
		char dummy1; char dummy2;
	};

	// exists always
	template<typename U>
	static InitIsMissingType is_here(...);

	// exist only if ExistsIfInitIsDefinedMarker is defined
	template<typename U>
	static InitExistsType is_here(ExistsIfInitIsDefinedMarker<U>*);

	enum { InitExists = (sizeof(is_here<T>(NULL)) == sizeof(InitExistsType)) };
};

template<typename T>
struct ParserDestroyExistsRecognizer {
	template<typename U, void (U::*)() = &U::Destroy>
	struct ExistsIfDestroyIsDefinedMarker{};

	struct DestroyIsMissingType {
		char dummy1;
	};
	
	struct DestroyExistsType {
		char dummy1; char dummy2;
	};

	// exists always
	template<typename U>
	static DestroyIsMissingType is_here(...);

	// exist only if ExistsIfDestroyIsDefinedMarker is defined
	template<typename U>
	static DestroyExistsType is_here(ExistsIfDestroyIsDefinedMarker<U>*);

	enum { DestroyExists = (sizeof(is_here<T>(NULL)) == sizeof(DestroyExistsType)) };
};

// The folloing templates are used to call the Init and Destroy methods if they exist.

// Generic case of the ParserInitCaller, gets used if the Init method is missing
template<typename T, bool = ParserInitExistsRecognizer<T>::InitExists>
struct ParserInitCaller {
	static void CallInit(T *t) {
		// nothing to do
	}
};

// True case of the ParserInitCaller, gets used if the Init method exists
template<typename T>
struct ParserInitCaller<T, true> {
	static void CallInit(T *t) {
		t->Init();
	}
};

// Generic case of the ParserDestroyCaller, gets used if the Destroy method is missing
template<typename T, bool = ParserDestroyExistsRecognizer<T>::DestroyExists>
struct ParserDestroyCaller {
	static void CallDestroy(T *t) {
		// nothing to do
	}
};

// True case of the ParserDestroyCaller, gets used if the Destroy method exists
template<typename T>
struct ParserDestroyCaller<T, true> {
	static void CallDestroy(T *t) {
		t->Destroy();
	}
};

void Parser::Parse() {
	t = NULL;
	la = dummyToken = new Token();
	la->val = coco_string_create(L"Dummy Token");
	Get();
	TNOO();
	Expect(0);
}

Parser::Parser(Scanner *scanner) {
	maxT = 66;

	ParserInitCaller<Parser>::CallInit(this);
	dummyToken = NULL;
	t = la = NULL;
	minErrDist = 2;
	errDist = minErrDist;
	this->scanner = scanner;
	errors = new Errors();
}

